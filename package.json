{
  "name": "asistente-caja-ande",
  "version": "0.0.1",
  "description": "Asistente virtual tipo chatbot para asistir a clientes de Caja Ande",
  "private": true,
  "license": "UNLICENSED",
  "author": "WIT",
  "contributors": [
    {
      "name": "Edixon Pi√±a",
      "email": "edixonalbertto@gmail.com"
    }
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier --write ./src/**/*.ts",
    "dev": "cross-env NODE_ENV=development nodemon --config",
    "build": "tsc && tsc-alias",
    "start": "npm run build && cross-env NODE_ENV=production node dist"
  },
  "nodemonConfig": {
    "ext": ".ts,.env",
    "watch": [
      "src",
      "env"
    ],
    "verbose": false,
    "exec": "npm run build && node dist"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "form-data": "^4.0.0",
    "https": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^1.19.1",
    "tsc-alias": "^1.4.1",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "engine": {
    "node": "^16.13.0",
    "npm": "^8.1.0"
  }
}
